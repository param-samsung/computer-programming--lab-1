#include <stdio.h>
void main() {
    int c;
    int add();
    int multiply();
   printf("addition is %d\n",add());
    printf("multilication is %d\n",multiply());
}
int add(){
    int x,y,z;
    printf("enter 2 values\n");
    scanf("%d %d",&x,&y);
    z=x+y;
    return z;
}
    int multiply(){
    int x,y,z;
    printf("enter 2 values\n");
    scanf("%d %d",&x,&y);
    z=x*y;
    return z;}
    
