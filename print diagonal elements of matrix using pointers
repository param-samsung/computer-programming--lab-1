#include <stdio.h>
int main() {
   int a[2][2];
printf(" matrix:\n");
for(int i=0;i<2;i++){
    for(int j=0;j<2;j++){
        scanf("%d",&a[i][j]);
    }}
    int *x[2];
    for(int i=0; i<2; i++) {
        x[i] = a[i];  
    }
    int **y=x;
    printf("diagonal elements are\n");
    for(int i=0;i<2;i++){
        printf("%d",y[i][i]);
    
    
}}
