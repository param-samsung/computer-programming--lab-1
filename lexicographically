#include <stdio.h>
#include <string.h> 

int main() {
    char a[100];
    int n;
    printf("enter the string: \n");
    fgets(a,100,stdin);
    n=strlen(a);
    for(int i=0;i<n-1;i++){
         for(int j=0;j<n-1-i;j++){
             if(a[j+1]<a[j]){
                 char x = a[j+1];
                 a[j+1]=a[j];
                 a[j]=x;
             }
         }
        
    }
    printf("%s",a);}
    
