#include <stdio.h>

int main() {
   int a[10];
   for(int i=0;i<10;i++){
   scanf("%d\n",&a[i]);}
   int c=0;
for(int i=0;i<10;i++){
    c=0;
    for(int j=i+1;j<10;j++){
    if(a[i]==a[j]){
        c+=1;
        a[j]=0;
        
    }}
    if(a[i]!=0){
    printf("%d occurs %d times\n",a[i],c+1);}} }
