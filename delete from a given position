#include <stdio.h>
int main() {
    int n;
    printf("Enter number of elements: ");
    scanf("%d", &n);
    int a[n],b[n-1],num,i,c;
    printf("Enter %d elements:\n", n);
    for(i = 0; i < n; i++) {
        scanf("%d", &a[i]);
    }
    printf("Enter position to delete from array : ");
    scanf("%d", &c);
    i=0;
    while(i<c){
        b[i]=a[i];
        i++;
    }
    while(i<n-1){
        b[i]=a[i+1];
        i++;
    }
    printf("Array after deletion:\n");
    for(i = 0; i < n-1; i++) {
        printf("%d ", b[i]);
    }
}
