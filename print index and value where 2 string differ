#include <stdio.h>

int main() {
   char a[10]={"print"},b[10]={"pritt"};
   char *p,*q;
   p=&a[0];
   q=&b[0];
   for(int i=0;i<10;i++){
       if(*p!=*q){
       printf("string differ at %d position and its value is %c,%c",i,*p,*q);
       break;
   }
   else{
       p++;
       q++;
   }
   }
   
}
