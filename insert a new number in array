#include <stdio.h>

int main() {
    int n;
    printf("Enter number of elements: ");
    scanf("%d", &n);
    int a[n],b[n+1],num, i;
    printf("Enter %d elements:\n", n);
    for(i = 0; i < n; i++) {
        scanf("%d", &a[i]);
    }
    printf("Enter number to insert at beginning: ");
    scanf("%d", &num);
    for(i = n; i > 0; i--) {
        b[i] = a[i - 1];
    }
    b[0] = num;
    printf("Array after insertion:\n");
    for(i = 0; i < n+1; i++) {
        printf("%d ", b[i]);
    }
}
